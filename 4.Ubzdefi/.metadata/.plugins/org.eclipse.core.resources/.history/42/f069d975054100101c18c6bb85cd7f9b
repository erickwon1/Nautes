package utils;

import java.io.File;
import java.io.FileWriter;

import lombok.AllArgsConstructor;

public class InputLog {

	public void inputLog() {
		File file = new File("");
		FileWriter writer = new FileWriter(file);
		String message;
		writer = new FileWriter(file, true);
		writer.write(message);
		writer.flush();
	}
}
